      state.</span><span><span class="hljs-property">streak</span></span><span> = </span><span><span class="hljs-number">0</span></span><span>; </span><span><span class="hljs-comment">// reset streak when manually changing deck</span></span><span>
      </span><span><span class="hljs-title function_">saveState</span></span><span>();
      </span><span><span class="hljs-title function_">updateDeckIndicator</span></span><span>();
      </span><span><span class="hljs-title function_">updateStats</span></span><span>();
