    </span><span><span class="hljs-keyword">if</span></span><span> (</span><span><span class="hljs-title class_">Number</span></span><span>.</span><span><span class="hljs-built_in">isFinite</span></span><span>(idx) &amp;&amp; idx &gt;= </span><span><span class="hljs-number">0</span></span><span> &amp;&amp; idx &lt; </span><span><span class="hljs-variable constant_">DECKS</span></span><span>.</span><span><span class="hljs-property">length</span></span><span>) {
